{
    "name": "redis",
    "version": "0.5.1",
    "publisher": "Dunn",
    "engines": {
        "vscode": "^1.22.0"
    },
    "license": "MIT",
    "main": "./lib/index.js",
    "displayName": "Redis",
    "description": "Redis For VS Code",
    "scripts": {
        "dev": "webpack-dev-server --config './config/webpack.ts' --config-name 'view'",
        "lint": "eslint '**/*.ts'",
        "lint:fix": "eslint --fix '**/*.ts'",
        "watch": "EXTENSION='watch' webpack --config './config/webpack.ts' --watch",
        "compile": "rm -rf ./lib/* && EXTENSION='production' webpack --config './config/webpack.ts'",
        "pre": "rm -rf ./lib && npm run compile",
        "vscode:prepublish": "npm run pre"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "redis",
        "tools",
        "manager"
    ],
    "galleryBanner": {},
    "preview": true,
    "contributors": [
        {
            "name": "Dunn",
            "email": "wg0121@outlook.com"
        }
    ],
    "badges": [],
    "icon": "resources/image/logo.png",
    "homepage": "https://github.com/pikadun/vscode-redis/blob/master/README.md",
    "bugs": {
        "url": "https://github.com/pikadun/vscode-redis/issues",
        "email": "wg0121@outlook.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/pikadun/vscode-redis"
    },
    "activationEvents": [
        "onView:Connection"
    ],
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "Connection",
                    "title": "Redis Explorer",
                    "icon": "resources/image/icon.png"
                }
            ]
        },
        "views": {
            "Connection": [
                {
                    "id": "Connection",
                    "name": "redis"
                }
            ]
        },
        "commands": [
            {
                "command": "Connection.Add",
                "title": "Add Connection",
                "icon": "resources/image/add.png"
            },
            {
                "command": "Connection.Delete",
                "title": "Delete Connection",
                "icon": "resources/image/delete.png"
            },
            {
                "command": "Connection.Edit",
                "title": "Edit Connection",
                "icon": "resources/image/add.png"
            },
            {
                "command": "Key.Detail",
                "title": "View Key Detail"
            },
            {
                "command": "DB.Reload",
                "title": "Reload Keys",
                "icon": "resources/image/refresh.png"
            },
            {
                "command": "DB.AddKey",
                "title": "Add Key"
            },
            {
                "command": "VR.Feedback",
                "title": "Feedback",
                "icon": "resources/image/feedback.png"
            },
            {
                "command": "Connection.Terminal",
                "title": "Open Terminal",
                "icon": "resources/image/terminal.png"
            }
        ],
        "menus": {
            "view/title": [
                {
                    "command": "Connection.Add",
                    "group": "navigation",
                    "when": "view == Connection"
                },
                {
                    "command": "VR.Feedback",
                    "group": "navigation",
                    "when": "view == Connection"
                }
            ],
            "view/item/context": [
                {
                    "command": "Connection.Delete",
                    "when": "viewItem == redis",
                    "group": "inline@2"
                },
                {
                    "command": "Connection.Terminal",
                    "when": "viewItem == redis",
                    "group": "inline@1"
                },
                {
                    "command": "DB.Reload",
                    "when": "viewItem == db",
                    "group": "inline"
                }
            ]
        }
    },
    "eslintConfig": {
        "extends": "./config/.eslintrc.json"
    },
    "devDependencies": {
        "@types/node": "^13.13.9",
        "@types/vscode": "^1.22.0",
        "@types/webpack": "^4.41.13",
        "@types/webpack-dev-server": "^3.11.0",
        "@types/webpack-merge": "^4.1.5",
        "@typescript-eslint/eslint-plugin": "^2.34.0",
        "@typescript-eslint/parser": "^2.34.0",
        "css-loader": "^3.5.3",
        "eslint": "^6.8.0",
        "html-webpack-plugin": "^4.3.0",
        "ts-loader": "^7.0.5",
        "ts-node": "^8.10.1",
        "typescript": "^3.9.3",
        "vue": "^2.6.11",
        "vue-loader": "^15.9.2",
        "vue-router": "^3.0.7",
        "vue-style-loader": "^4.1.2",
        "vue-template-compiler": "^2.6.11",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.11.0",
        "webpack-merge": "^4.2.2"
    },
    "dependencies": {
        "redis-parser": "^3.0.0"
    }
}