{
    "name": "redis",
    "version": "0.8.1",
    "publisher": "Dunn",
    "engines": {
        "vscode": "^1.46.0"
    },
    "license": "MIT",
    "main": "./lib/index.js",
    "displayName": "Redis",
    "description": "Redis For VS Code",
    "scripts": {
        "dev": "webpack-dev-server --mode=development --config=./config/webpack.ts --config-name=view",
        "lint": "eslint '**/*.ts'",
        "lint:fix": "eslint --fix '**/*.ts'",
        "watch": "webpack --mode=none --config=./config/webpack.ts --watch",
        "compile": "rm -rf ./lib && webpack --mode=production --config=./config/webpack.ts",
        "vscode:prepublish": "npm run compile"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "redis",
        "redis cli",
        "redis gui",
        "terminal",
        "manager"
    ],
    "galleryBanner": {},
    "preview": true,
    "contributors": [
        {
            "name": "Dunn",
            "email": "wg0121@outlook.com"
        }
    ],
    "badges": [],
    "icon": "resources/image/logo.png",
    "homepage": "https://github.com/pikadun/vscode-redis/blob/master/README.md",
    "bugs": {
        "url": "https://github.com/pikadun/vscode-redis/issues",
        "email": "wg0121@outlook.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/pikadun/vscode-redis"
    },
    "activationEvents": [
        "onView:RedisExplorer"
    ],
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "RedisExplorer",
                    "title": "Redis Explorer",
                    "icon": "resources/image/icon.png"
                }
            ]
        },
        "views": {
            "RedisExplorer": [
                {
                    "id": "RedisExplorer",
                    "name": "redis"
                }
            ]
        },
        "commands": [
            {
                "command": "Redis.Connection.Add",
                "title": "Add Connection",
                "icon": "$(add)"
            },
            {
                "command": "Redis.Connection.Delete",
                "title": "Delete Connection",
                "icon": "$(trashcan)"
            },
            {
                "command": "Redis.Connection.Edit",
                "title": "Edit Connection",
                "icon": "$(add)"
            },
            {
                "command": "Redis.Key.Detail",
                "title": "View Key Detail"
            },
            {
                "command": "Redis.DB.Reload",
                "title": "Reload Keys",
                "icon": "$(refresh)"
            },
            {
                "command": "Redis.DB.Search",
                "title": "Keys Filter",
                "icon": "$(search)"
            },
            {
                "command": "Redis.DB.AddKey",
                "title": "Add Key"
            },
            {
                "command": "Redis.Feedback",
                "title": "Feedback",
                "icon": "$(feedback)"
            },
            {
                "command": "Redis.Star",
                "title": "Star on github",
                "icon": "$(github-alt)"
            },
            {
                "command": "Redis.Terminal",
                "title": "Open Terminal",
                "icon": "$(terminal)"
            }
        ],
        "menus": {
            "view/title": [
                {
                    "command": "Redis.Connection.Add",
                    "group": "navigation@1",
                    "when": "view == RedisExplorer"
                },
                {
                    "command": "Redis.Feedback",
                    "group": "navigation@3",
                    "when": "view == RedisExplorer"
                },
                {
                    "command": "Redis.Star",
                    "group": "navigation@2",
                    "when": "view == RedisExplorer"
                }
            ],
            "view/item/context": [
                {
                    "command": "Redis.Connection.Delete",
                    "when": "viewItem == redis",
                    "group": "inline@2"
                },
                {
                    "command": "Redis.Terminal",
                    "when": "viewItem == redis",
                    "group": "inline@1"
                },
                {
                    "command": "Redis.DB.Reload",
                    "when": "viewItem == db",
                    "group": "inline@2"
                },
                {
                    "command": "Redis.DB.Search",
                    "when": "viewItem == db",
                    "group": "inline@1"
                }
            ]
        }
    },
    "eslintConfig": {
        "extends": "./config/.eslintrc.json"
    },
    "devDependencies": {
        "@types/node": "^14.0.14",
        "@types/vscode": "^1.46.0",
        "@types/webpack": "^4.41.18",
        "@types/webpack-dev-server": "^3.11.0",
        "@types/webpack-merge": "^4.1.5",
        "@typescript-eslint/eslint-plugin": "^3.4.0",
        "@typescript-eslint/parser": "^3.4.0",
        "css-loader": "^3.6.0",
        "eslint": "^7.3.1",
        "html-webpack-plugin": "^4.3.0",
        "redis-parser": "^3.0.0",
        "ts-loader": "^7.0.5",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.5",
        "vue": "^2.6.11",
        "vue-loader": "^15.9.3",
        "vue-router": "^3.3.4",
        "vue-style-loader": "^4.1.2",
        "vue-template-compiler": "^2.6.11",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0",
        "webpack-merge": "^4.2.2"
    }
}
