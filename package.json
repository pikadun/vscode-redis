{
    "name": "redis",
    "version": "0.1.4",
    "publisher": "Dunn",
    "engines": {
        "vscode": "^1.43.0"
    },
    "license": "MIT",
    "main": "./lib/index.js",
    "displayName": "Redis",
    "description": "Redis For VS Code",
    "categories": [
        "Other"
    ],
    "keywords": [
        "redis",
        "tools",
        "manager"
    ],
    "galleryBanner": {},
    "preview": true,
    "contributors": [
        {
            "name": "Dunn",
            "email": "wg0121@outlook.com"
        }
    ],
    "badges": [],
    "icon": "resources/image/logo.png",
    "homepage": "https://github.com/pikadun/vscode-redis/blob/master/README.md",
    "bugs": {
        "url": "https://github.com/pikadun/vscode-redis/issues",
        "email": "wg0121@outlook.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/pikadun/vscode-redis"
    },
    "scripts": {
        "lint": "eslint '**/*.ts'",
        "lint:fix": "eslint --fix '**/*.ts'",
        "compile": "tsc --build tsconfig.json",
        "vscode:prepublish": "npm run compile"
    },
    "activationEvents": [
        "onView:Connection"
    ],
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "Connection",
                    "title": "Redis Explorer",
                    "icon": "resources/image/icon.png"
                }
            ]
        },
        "views": {
            "Connection": [
                {
                    "id": "Connection",
                    "name": "redis"
                }
            ]
        },
        "commands": [
            {
                "command": "Connection.Add",
                "title": "Add Connection",
                "icon": "resources/image/add.png"
            },
            {
                "command": "Connection.Delete",
                "title": "Delete Connection",
                "icon": "resources/image/delete.png"
            },
            {
                "command": "Key.Detail",
                "title": "View Key Detail"
            },
            {
                "command": "DB.AddKey",
                "title": "Add Key"
            }
        ],
        "menus": {
            "view/title": [
                {
                    "command": "Connection.Add",
                    "group": "navigation"
                }
            ],
            "view/item/context": [
                {
                    "command": "Connection.Delete",
                    "when": "viewItem == redis",
                    "group": "inline"
                }
            ]
        }
    },
    "devDependencies": {
        "@types/node": "^13.9.5",
        "@types/vscode": "^1.43.0",
        "@typescript-eslint/eslint-plugin": "^2.31.0",
        "@typescript-eslint/parser": "^2.31.0",
        "eslint": "^6.8.0",
        "typescript": "^3.8.3"
    },
    "dependencies": {
        "redis-parser": "^3.0.0"
    }
}
